n=int(input("enter the number:"))
def factorial(n):
        if n<2:
            return n
        else :
            return n *factorial(n-1)
result=factorial(n)
print (result)

n=int(input("enter the number:"))
from math import*
square=sqrt(n)
log=log(n,e)
sin=sin(n)
print(square)
print(log)
print(sin)
